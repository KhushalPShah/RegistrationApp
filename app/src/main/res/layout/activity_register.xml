<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.android.test_application.Register"
    >


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="NAME:"
        android:id="@+id/textView_registered_name"
        android:textStyle="bold"
         />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NAME OF STUDENT"
        android:id="@+id/textView_registered_name_input"
        android:layout_below="@+id/textView_registered_name"
        android:layout_marginTop="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="COLLEGE NAME:"
        android:id="@+id/textView_registered_college_name"
        android:layout_below="@+id/textView_registered_name_input"
        android:layout_marginTop="5dp"
        android:textStyle="bold"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NAME OF COLLEGE"
        android:id="@+id/textView_registered_college_name_input"
        android:layout_below="@+id/textView_registered_college_name"
        android:layout_marginTop="5dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="COLLEGE ADDRESS:"
        android:id="@+id/textView_registered_college_address"
        android:layout_below="@+id/textView_registered_college_name_input"
        android:layout_marginTop="5dp"
        android:textStyle="bold"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADDRESS OF COLLEGE"
        android:id="@+id/textView_registered_college_address_input"
        android:layout_below="@+id/textView_registered_college_address"
        android:layout_marginTop="5dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PHONE NUMBER:"
        android:id="@+id/textView_registered_phone_number"
        android:layout_below="@+id/textView_registered_college_address_input"
        android:layout_marginTop="5dp"
        android:textStyle="bold"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PHONE NUMBER OF PARTICIPANT"
        android:id="@+id/textView_registered_phone_number_input"
        android:layout_below="@+id/textView_registered_phone_number"
        android:layout_marginTop="5dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EMAIL-ID:"
        android:id="@+id/textView_registered_email"
        android:layout_below="@+id/textView_registered_phone_number_input"
        android:layout_marginTop="5dp"
        android:textStyle="bold"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EMAIL-ID OF THE PARTICIPANT"
        android:id="@+id/textView_registered_email_input"
        android:layout_below="@+id/textView_registered_email"
        android:layout_marginTop="5dp"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CANCEL"
        android:id="@+id/button3"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:onClick="return_to_main"
        android:textStyle="bold"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONFIRM"
        android:id="@+id/button4"
        android:layout_above="@+id/button3"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp"
        android:onClick="confirm"
        android:textStyle="bold"/>


</RelativeLayout>
